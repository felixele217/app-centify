{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "devio/pipedrive": "^2.16",
        "fakerphp/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.8",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "omniphx/forrest": "^2.16",
        "spatie/laravel-data": "^3.7",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "doctrine/dbal": "^3.6",
        "laravel/breeze": "^1.21",
        "laravel/dusk": "^7.9",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.23",
        "mockery/mockery": "^1.4.4",
        "mudandstars/sync-enum-types": "^1.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0",
        "pestphp/pest": "^2.9",
        "pestphp/pest-plugin-laravel": "^2.1",
        "pestphp/pest-plugin-type-coverage": "^2.0",
        "spatie/laravel-ignition": "^2.0",
        "worksome/request-factories": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "prettier": "npx prettier --write ./resources/js",
        "pint": "vendor/bin/pint",
        "format": "composer prettier && composer pint",
        "test-local": "composer pest-local && composer vitest && composer dusk-local",
        "pest-local": "vendor/bin/pest --group=feature,unit,architecture",
        "vitest": "npm run test",
        "dusk-local": "bash ./scripts/killViteProcess.sh && npm run build && sail dusk",
        "stan": "vendor/bin/phpstan analyse --memory-limit=2G"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
